import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int x,y,x_,y_;
		int x1,x2,y1,y2;
		x = 10;
		x_ = 200;
		y = 10;
		y_ = 300;
		
		x1 = scanner.nextInt();
		y1 = scanner.nextInt();
		x2 = scanner.nextInt();
		y2 = scanner.nextInt();
		
		boolean isIn;
		
		if(x1 >= x && x2 <= x_) {
			if(y1>= y && y2 <= y_)
				isIn = true;
			else
				isIn = false;
		}
		else
			isIn = false;
		System.out.println(isIn);
		scanner.close();
	}
}
